{
  "name": "is-api-middleware-resolver",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@vtex/prettier-config": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-vtex": "^15.0.2",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "prettier": "^2.8.4",
    "typescript": "^3.7.5"
  }
}
